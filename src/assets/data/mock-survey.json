{
  "title": "Some test survey",
  "controls": [
    {
      "name": "firstName",
      "label": "First name:",
      "defaultValue": "",
      "type": "text",
      "validators": {
        "required": true,
        "minLength": 4
      }
    },
    {
      "name": "lastName",
      "label": "Last name:",
      "defaultValue": "",
      "type": "text",
      "validators": {}
    },
    {
      "name": "password",
      "label": "Password",
      "defaultValue": "",
      "type": "password",
      "validators": {
        "required": true,
        "minLength": 6,
        "maxLength": 22
      }
    },
    {
      "name": "email",
      "label": "Email",
      "defaultValue": "",
      "type": "email",
      "validators": {
        "required": true,
        "email": true
      }
    },
    {
      "name": "comments",
      "label": "Comments",
      "defaultValue": "",
      "type": "textarea",
      "validators": {}
    },
    {
      "name": "agreeToTerms",
      "label": "This is a checkbox?",
      "defaultValue": false,
      "type": "checkbox",
      "validators": {
        "requiredTrue": true
      }
    },
    {
      "name": "size",
      "label": "Range label",
      "defaultValue": 0,
      "type": "range",
      "options": {
        "min": "0",
        "max": "100",
        "step": "1",
        "icon": "sunny"
      },
      "validators": {}
    },
    {
      "name": "lightDark",
      "label": "Do you like toggles?",
      "defaultValue": false,
      "type": "toggle",
      "validators": {}
    },
    {
      "name": "singleChoice",
      "label": "Single Choice Question",
      "defaultValue": "Yes",
      "choices": [
        "Yes",
        "No",
        "Maybe"
      ],
      "type": "radio",
      "validators": {}
    },
    {
      "name": "multipleChoice",
      "label": "Multiple choice",
      "defaultValue": "",
      "choices": [
        "Apples",
        "Oranges",
        "Bananas"
      ],
      "type": "multiple-choice",
      "validators": {}
    }
  ]
}
